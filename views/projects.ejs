<!DOCTYPE>

<html>
<head>

    <link rel="stylesheet" href="/assets/css/style.css">

</head>


<body>

    <h1>List of projects</h1>


    <% for(var i=0;i<projects.length;i++){ %>

    <div class="project-container">
        <h3><%- projects[i].title %></h3>
        <p><%- projects[i].content %></p>
        <p><%- projects[i].dateCreated %></p>
    </div>
    <hr/>

    <% } %>

</body>

</html>

